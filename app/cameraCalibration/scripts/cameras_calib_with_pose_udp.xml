<application>
<name>Calib Cameras with pose UDP</name>
<dependencies>
</dependencies> 
<module>
  <name>yarpdev</name>
      <parameters> --from camera/dragonfly2_config_left.ini</parameters>
  <node>pc104</node>
</module>
<module>
  <name>yarpdev</name>
      <parameters> --from camera/dragonfly2_config_right.ini</parameters>
  <node>pc104</node>
</module>
<module>
  <name>camCalibWithPose</name>
  <parameters>--context cameraCalibration --from icubEyes_640x480.ini --group CAMERA_CALIBRATION_LEFT --name /icub/camcalibPose/left</parameters>  
  <node>localhost</node>
</module>
<module>
  <name>camcalibWithPose</name>
  <parameters>--context cameraCalibration --from icubEyes_640x480.ini --group CAMERA_CALIBRATION_RIGHT --name /icub/camcalibPose/right</parameters> 
  <node>localhost</node>
</module>
<module>
  <name>yarpview</name>
  <parameters>--name /icub/randazview/left --x 0 --y 0 --p 50 --compact</parameters>
  <node>localhost</node>
</module>
<module>
  <name>yarpview</name>
  <parameters>--name /icub/randazview/right --x 320 --y 0 --p 50 --compact</parameters>
  <node>localhost</node>
</module>
<module>
  <name>frameGrabberGui2</name>
  <parameters>--local /icub/fggui/left --remote /icub/cam/left --x 0 --y 350 --width 350 --height 500</parameters>
  <node>icub16</node>
  <dependencies>
     <port timeout="10">/icub/cam/left</port>
  </dependencies>
</module>
<module>
  <name>frameGrabberGui2</name>
  <parameters>--local /icub/fggui/right --remote /icub/cam/right --x 320 --y 350 --width 350 --height 500</parameters>
  <node>icub16</node>
  <dependencies>
     <port timeout="10">/icub/cam/right</port>
  </dependencies>
</module>
<connection>
  <from>/icub/cam/left</from>
  <to>/icub/camcalibPose/left/in</to>
  <protocol>udp</protocol>
</connection>
<connection>
  <from>/icub/cam/right</from>
  <to>/icub/camcalibPose/right/in</to>
  <protocol>udp</protocol>
</connection>
<connection>
  <from>/icub/inertial</from>
  <to>/icub/camcalibPose/left/imu/in</to>
  <protocol>udp</protocol>
</connection>
<connection>
  <from>/icub/inertial</from>
  <to>/icub/camcalibPose/right/imu/in</to>
  <protocol>udp</protocol>
</connection>
<connection>
  <from>/icub/head/state:o</from>
  <to>/icub/camcalibPose/right/head_encs/in</to>
  <protocol>udp</protocol>
</connection>
<connection>
  <from>/icub/torso/state:o</from>
  <to>/icub/camcalibPose/right/torso_encs/in</to>
  <protocol>udp</protocol>
</connection>
<connection>
  <from>/icub/head/state:o</from>
  <to>/icub/camcalibPose/left/head_encs/in</to>
  <protocol>udp</protocol>
</connection>
<connection>
  <from>/icub/torso/state:o</from>
  <to>/icub/camcalibPose/left/torso_encs/in</to>
  <protocol>udp</protocol>
</connection>
<connection>
  <from>/icub/inertial</from>
  <to>/icub/camcalibPose/left/imu/in</to>
  <protocol>udp</protocol>
</connection>
<connection>
  <from>/icub/camcalibPose/left/out</from>
  <to>/icub/randazview/left</to>
  <protocol>udp</protocol>
</connection>
<connection>
  <from>/icub/camcalibPose/right/out</from>
  <to>/icub/randazview/right</to>
  <protocol>udp</protocol>
</connection>
</application>

